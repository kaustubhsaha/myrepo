<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Works on the basis of the facts that : -->
	<!-- Foo has a constructor which takes argument of type Bar -->
	<!-- There are multiple beans of type Bar available for injection but one is marked as primary -->
	<bean id="foo" class="spring.xml.ioc.autowiring.Foo" autowire="constructor"/>
	<bean id="bar" class="spring.xml.ioc.autowiring.Bar" primary="true"/>
	<bean id="barbar" class="spring.xml.ioc.autowiring.Bar"/>
	
</beans>
